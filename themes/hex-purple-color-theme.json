{
  "name": "Hex Purple",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "activityBarBadge.background": "#C792EA",
    "sideBarTitle.foreground": "#bbbbbb",
    "terminal.background": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#697098"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Control Flow Keywords",
      "scope": [
        "constant.language",
        "keyword.control.flow",
        "storage.modifier.async",
        "storage.type.await"
      ],
      "settings": {
        "foreground": "#ff5e2d"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#bdcff5"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F07178"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    }
  ]
}
